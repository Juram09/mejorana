<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="245.0" prefWidth="535.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="logic.BackupController">
   <children>
      <ImageView fitHeight="275.0" fitWidth="535.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imgs/backup.png" />
         </image>
      </ImageView>
      <Button fx:id="exitBtn" layoutX="486.0" layoutY="11.0" mnemonicParsing="false" onAction="#exit" opacity="0.01" prefHeight="39.0" prefWidth="41.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="importBtn" layoutX="59.0" layoutY="101.0" mnemonicParsing="false" onAction="#imprt" opacity="0.01" prefHeight="46.0" prefWidth="417.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="exportBtn" layoutX="59.0" layoutY="160.0" mnemonicParsing="false" onAction="#export" opacity="0.01" prefHeight="46.0" prefWidth="417.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="244.0" layoutY="225.0" text="Ver Logs" textFill="#089eff" underline="true" />
      <Button fx:id="logsBtn" layoutX="244.0" layoutY="221.0" mnemonicParsing="false" onAction="#viewLogs" opacity="0.01" prefHeight="25.0" prefWidth="47.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
