            <Font name="Century Gothic" size="16.0" />
         </font>
      </TextField>
      <Button fx:id="addBtn" layoutX="348.0" layoutY="332.0" mnemonicParsing="false" onAction="#addTanq" opacity="0.01" prefHeight="30.0" prefWidth="262.0">
